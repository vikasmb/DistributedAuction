<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/BuyerCriteria.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>BuyerCriteria.xhtml</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>viewAuctions</from-outcome>
			<to-view-id>/ViewAuctions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>BuyerCriteria.xhtml</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>viewSubscriptions</from-outcome>
			<to-view-id>/Subscriptions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ViewAuctions.xhtml</display-name>
		<from-view-id>/ViewAuctions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goHome</from-outcome>
			<to-view-id>/BuyerCriteria.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Subscriptions.xhtml</display-name>
		<from-view-id>/Subscriptions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goHome</from-outcome>
			<to-view-id>/BuyerCriteria.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
